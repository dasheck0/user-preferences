<templateSet group="JavaScript">
  <template name="log" value="console.log($END$);" description="Console log" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="winreq" value="const winston = require('$END$');" description="Require winston" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="cls" value="class $NAME$ {&#10;}&#10;&#10;module.exports = $NAME$;" description="Class boilerplate" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="scls" value="let instance = null;&#10;&#10;class $NAME$ {&#10;    constructor() {&#10;        if(!instance) {&#10;            instance = this;&#10;        }&#10;        &#10;        return instance;&#10;    }&#10;}&#10;&#10;module.exports = new $NAME$();" description="Singleton class boilerplate" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="info" value="console.info($END$);" description="Console info" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>